<div class="haikei">
<div class="container  blog border-left border-right border-dark">
  <div class="row">
    <div class="col">
      <h2>New Memory</h2>
      <%= form_with model: @blog, local:true do |f| %>
        <h4>画像</h4>
        <%= f.attachment_field :image %>
        <h4>タイトル</h4>
        <%= f.text_field :title %>
        <h4>説明</h4>
        <%= f.text_area :description %>
        <h4>国</h4>
        <%= f.country_select :country_name, priority_countries: ["JP", "US"] %>
        <%= f.submit '投稿' %>
      <% end %>
    </div>
  </div>
</div>
</div>